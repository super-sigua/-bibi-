#include <stdio.h>
int main()
{
	int hour1 = 0, minute1 = 0;
	int hour2 = 0, minute2 = 0;
	printf("请输入时间小时分钟中间以空格隔开:\n");
	scanf("%d %d",&hour1, &minute1);
	printf("请输入第二个时间，小时分钟中间以空格隔开：\n");
	scanf("%d %d",&hour2, &minute2);
	unsigned int zhuanhua1 =0;
	unsigned int zhuanhua2 =0;
	zhuanhua1 = hour1*60+minute1;
	zhuanhua2 = hour2*60+minute2;
	printf("%d", zhuanhua1-zhuanhua2);
	return 0;
}
